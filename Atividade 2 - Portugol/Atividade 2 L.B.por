programa {
  funcao inicio() {
    //Declaração de variavéis
    cadeia nome, sobrenome

    //Entrada de dados
    escreva ("Informe seu nome: ")
    leia (nome)
    escreva ("Informe seu sobrenome: ")
    leia (sobrenome)

    //Saída de dados
    escreva ("Formato padrão brasileiro: ", nome, " ", sobrenome, "\n")
    escreva ("Formato invertido: ", sobrenome, " ", nome, "\n")
  }
}
