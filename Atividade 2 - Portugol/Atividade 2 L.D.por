programa {
  funcao inicio() {
    //Declaração de variávis
    real temperaturaCel, temperaturaFar

    //Entrada de dados
    escreva ("Informe a temperatura: ")
    leia (temperaturaCel)

    //Processamento
    temperaturaFar = (9 * temperaturaCel + 160)/5

    //Saída de daddos
    escreva (temperaturaCel, " em graus Fahrenheit é : ", temperaturaFar, ".")
  }
}
