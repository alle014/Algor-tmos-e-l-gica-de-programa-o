programa {
  funcao inicio() {
    //Declaração de variáveis
    real altura, pesoideal

    //Entrada de dados
    escreva ("Informe sua altura : ")
    leia (altura)

    //Processamento
    pesoideal = (72.7 * altura) - 58

    //saída de dados
    escreva ("Seu peso ideal é : ", pesoideal)

  }
}
