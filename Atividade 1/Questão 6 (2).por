programa {
  funcao inicio() {
    //Declaração de variáveis
    real x, y, xtroa, ytroca

    //Entrada de dados
    escreva ("Informe um valor para X : ")
    leia (x)
    escreva ("Informe um valor para Y : ")
    leia (y)

    //Processamento
    xtroa = y 
    ytroca = x

    //Saída de dados
    escreva ("O valor de X é : ", xtroa, "\n", "O valor de Y é : ", ytroca)


  }
}
